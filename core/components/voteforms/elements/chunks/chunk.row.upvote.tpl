<div data-record="[[+record]]" data-id="[[+id]]" class="vtf__upvote">
    <a class="upvote [[+upvoteOn]]">
        <svg xmlns="http://www.w3.org/2000/svg" version="1.1"  viewBox="0 0 100 100"  class="svg-triangle">
            <polygon points="0,50 50,0 100,50"/>
        </svg>
    </a>
    <span class="icon [[+properties.class]]"></span>
    <span class="count">[[+rating]]</span>
    <a class="downvote [[+downvoteOn]]">
        <svg xmlns="http://www.w3.org/2000/svg" version="1.1" viewBox="0 0 100 100" class="svg-triangle">
            <polygon points="0,0 100,0 50,50"/>
        </svg>
    </a>
</div>